<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="500.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ma.game.tictactoemultiplayer.Controllers.LoginController">
   <children>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="150.0" layoutY="14.0" prefHeight="20.0" prefWidth="100.0" text="Log In" textAlignment="CENTER">
         <font>
            <Font name="System Bold" size="20.0" />
         </font>
      </Label>
      <ImageView fx:id="image" fitHeight="100.0" fitWidth="100.0" layoutX="150.0" layoutY="75.0" pickOnBounds="true" preserveRatio="true" />
      <Label layoutX="49.0" layoutY="234.0" prefHeight="30.0" prefWidth="90.0" text="Username">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Label layoutX="49.0" layoutY="292.0" prefHeight="30.0" prefWidth="90.0" text="Password">
         <font>
            <Font size="14.0" />
         </font></Label>
      <Button layoutX="150.0" layoutY="369.0" mnemonicParsing="false" onAction="#login" prefHeight="40.0" prefWidth="100.0" style="-fx-background-color: #ffc107;" text="Log In">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
      <TextField fx:id="username" layoutX="143.0" layoutY="235.0" prefHeight="30.0" prefWidth="200.0" />
      <PasswordField fx:id="password" layoutX="143.0" layoutY="292.0" prefHeight="30.0" prefWidth="200.0" />
      <Label alignment="CENTER_RIGHT" layoutX="111.0" layoutY="455.0" prefHeight="30.0" prefWidth="100.0" text="Not a user?" />
      <Button fx:id="signupBTN" layoutX="211.0" layoutY="455.0" onAction="#goToSignUp" prefHeight="30.0" prefWidth="53.0" style="-fx-background-color: transparent;" text="Signup" textFill="BLUE">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor></Button>
   </children>
</AnchorPane>
